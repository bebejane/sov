{
	"expo": {
		"name": "S o V",
		"slug": "sov",
		"version": "1.0.2",
		"owner": "bebedigital",
		"orientation": "portrait",
		"icon": "./assets/icons/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"backgroundColor": "#FFFFFF",
			"image": "./assets/icons/splash-icon.png",
			"imageWidth": 400,
			"ios": {
				"image": "./assets/icons/splash-icon.png",
				"resizeMode": "contain",
				"backgroundColor": "#FFFFFF"
			},
			"android": {
				"image": "./assets/icons/splash-icon.png",
				"resizeMode": "contain",
				"backgroundColor": "#FFFFFF"
			}
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "org.kt.sov",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icons/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "org.kt.sov"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/icons/favicon.png"
		},
		"plugins": ["expo-router", "expo-splash-screen"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "2dcbd8a8-f808-4a19-85fb-eb2b0e3fec5a"
			}
		}
	}
}
