{
	"expo": {
		"name": "S o V",
		"slug": "sov",
		"version": "1.0.4",
		"owner": "red_phalarope",
		"orientation": "portrait",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"icon": "./assets/icons/icon.png",
		"ios": {
			"icon": {
				"dark": "./assets/icons/icon.png",
				"light": "./assets/icons/icon.png",
				"tinted": "./assets/icons/icon-tinted.png"
			},
			"supportsTablet": true,
			"bundleIdentifier": "org.name.sov",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false,
				"UIBackgroundModes": ["audio"]
			}
		},
		"android": {
			"icon": "./assets/icons/icon-android.png",
			"adaptiveIcon": {
				"foregroundImage": "./assets/icons/adaptive-icon.png",
				"backgroundColor": "#015154"
			},
			"package": "org.redphalarope.sov",
			"versionCode": 2,
			"permissions": ["android.permission.WAKE_LOCK"]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/icons/favicon.png"
		},

		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"imageWidth": 200,
					"resizeMode": "contain",
					"ios": {
						"image": "./assets/icons/splash-icon.png",
						"resizeMode": "contain",
						"backgroundColor": "#FFFFFF",
						"imageWidth": 200
					},
					"android": {
						"image": "./assets/icons/splash-icon-android.png",
						"backgroundColor": "#FFFFFF",
						"imageWidth": 200
					}
				}
			],
			[
				"expo-build-properties",
				{
					"android": {
						"compileSdkVersion": 35,
						"targetSdkVersion": 35,
						"buildToolsVersion": "35.0.0"
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "1f1b42b5-4afb-4616-a49a-937765d8cf5f"
			}
		}
	}
}
